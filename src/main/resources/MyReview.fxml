<AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="600.0" stylesheets="@css/myReview.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MyReviewController">
   <children>
      <VBox prefHeight="200.0" prefWidth="891.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" minWidth="891.0" prefHeight="200.0" prefWidth="891.0" text="My Review:" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="157.0" prefWidth="891.0" spacing="10.0">
               <children>
                  <Label fx:id="contentReview" alignment="CENTER" prefHeight="194.0" prefWidth="240.0" styleClass="content" stylesheets="@css/reviewItem.css" text=" ContentPlaceholder" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Consolas Italic" size="8.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="10.0" />
                     </HBox.margin>
                  </Label>
                  <Button fx:id="editButton" mnemonicParsing="false" onAction="#editMyReview" stylesheets="@css/buttons.css" text="EDIT" />
                  <Button fx:id="deleteButton" layoutX="413.0" layoutY="78.0" mnemonicParsing="false" onAction="#deleteMyReview" stylesheets="@css/buttons.css" text="DELETE" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
